1720 GOSUB 100:GOTO 1530:REM begin opnieuw
1730 REM 
1740 REM subroutine tekst -> tekening
1750 REM 
1760 B=0:REM wel tekenen
1770 LE=LEN(TX$)
1780 IF LE=1 THEN 1820
1790 IF MID$(TX$,LE-1,1)="O" THEN GOSUB 3380
1800 IF LE=2 THEN 1820
1810 IF MID$(TX$,LE-2,1)="O" THEN GOSUB 3390
1820 FOR I=1 TO LE
1830 TY$=MID$(TX$,I,1)
1840 IF TY$="U" THEN PH=0:PV=-1:GOTO 1950
1850 IF TY$="D" THEN PH=0:PV=1:GOTO 1950
1860 IF TY$="L" THEN PH=-1:PV=0:GOTO 1950
1870 IF TY$="R" THEN PH=1:PV=0:GOTO 1950
1880 IF TY$="E" THEN PH=1:PV=-1:GOTO 1950
1890 IF TY$="F" THEN PH=1:PV=1:GOTO 1950
1900 IF TY$="G" THEN PH=-1:PV=1:GOTO 1950
1910 IF TY$="H" THEN PH=-1:PV=-1:GOTO 1950
1920 IF TY$="M" THEN 2080:REM grote afstand
1930 B=1:REM "B" = niet tekenen
1940 GOTO 2030:REM foute letter
1950 GOSUB 2170:REM hoe vaak?
1960 PH=PH*V9*SC:PV=PV*V9*SC:REM stappen
1970 X=X+PH:Y=Y+PV:REM stappen erbij
1980 HO=X/HA:VE=Y/VA:REM op scherm
1990 IF HO>=1 THEN 2020
2000 IF B=1 THEN CN=1:GOSUB 620:CN=0:GOTO 2020
2010 GOSUB 630:REM lijntje
2020 B=0:REM aansluiten op dit punt
2030 NEXT I:REM volgende letter
2040 RETURN:REM klaar met tekst
2050 REM 
2060 REM verwerking Move
2070 REM 
2080 GOSUB 2170:REM x coordinaat
2090 X=V9:REM toekennen
2100 I=I+1:REM komma overslaan
2110 GOSUB 2170:REM y coordinaat
2120 Y=V9:REM toekennen
2130 GOTO 1980:REM naar scherm
2140 REM 
2150 REM subroutine aantal malen
2160 REM 
2170 V9=0:REM nul malen
2180 IF LE=I THEN 2250:REM einde tekst
2190 TZ$=MID$(TX$,I+1,1):REM achter teken
2200 IF TZ$<"0" THEN 2250:REM geen
2210 IF TZ$>"9" THEN 2250:REM cijfer
2220 V9=V9*10+VAL(TZ$):REM meerdere cijfers
2230 I=I+1:REM een teken verder
2240 GOTO 2180:REM weer een cijfer?
2250 IF V9=0 THEN V9=1:REM minimum
2260 RETURN:REM klaar
