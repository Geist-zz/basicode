1000 A=100:GOTO 20:REM Breuken vereenvoudigen
1010 REM * LEES BREUK IN *
1020 PRINT "Teller : ";:INPUT TE:PE=TE
1040 PRINT "Noemer : ";:INPUT NO
1060 H=INT(TE/NO):R=TE-H*NO:TE=R:IF R=0 THEN 1300
1080 C=TE:D=NO
1090 Q=C-INT(C/D)*D:C=D:D=Q:IF Q>0 THEN 1090
1110 HO=0:VE=10:GOSUB 110
1120 PRINT "  Vereenvoudigd: "
1130 CT=LOG(NO/C)/LOG(10)+1:CN=0
1140 CQ=1:IF H>0 THEN CQ=LOG(H)/LOG(10)+1
1160 CQ$=LEFT$("         ",CQ)
1170 CT=LOG(NO/C)/LOG(10)+1
1180 PRINT 
1190 PRINT "  TELLER: ";CQ$
1200 PRINT "  ------- ";CQ$;"  ";
1210 PRINT LEFT$("-------------",CT)
1220 PRINT "  NOEMER: ";CQ$
1230 HO=12+CQ:VE=12:GOSUB 110:SR=TE/C:GOSUB 310:PRINT SR$
1240 VE=14:GOSUB 110:SR=NO/C:GOSUB 310:PRINT SR$
1250 IF H>0 THEN HO=11:VE=13:GOSUB 110:PRINT H
1260 HO=15+CT+CQ:VE=12:GOSUB 110
1270 PRINT "   (=";TE;" :";C;")"
1280 VE=14:GOSUB 110
1290 PRINT "   (=";NO;" :";C;")"
1300 HO=2:VE=16:GOSUB 110:PRINT "Breuk heeft waarde ";PE/NO
1310 HO=0:VE=19:GOSUB 110
1320 PRINT "Nog een (J/N) ?";
1330 GOSUB 210
1340 IF IN=74 THEN 1000
1350 IF IN<>78 THEN 1330
1360 GOTO 950
