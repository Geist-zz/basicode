2030 REM * UITLEG BIJ HET PROGRAMMA  *
2040 REM *****************************
2050 REM
2060 GOSUB100
2070 HO=0:VE=10:GOSUB110
2080 PRINT"   Bevolkingsprognose voor Nederland"
2090 PRINT"   ---------------------------------"
2100 FORI=0 TO3000:NEXTI
2110 GOSUB100
2120 PRINT"Dit programma berekent de mogelijke"
2130 PRINT"toekomstige bevolkingsontwikkeling voor"
2140 PRINT"Nederland."
2150 PRINT"Als startgegevens hiervoor zijn de zgn."
2160 PRINT"statische en dynamische bevolkings-"
2170 PRINT"gegevens van het jaar ";J0;" genomen."
2180 PRINT
2190 PRINT"Voordat het programma met rekenen"
2200 PRINT"begint kunt u nog invloed uitoefenen"
2210 PRINT"op de waarde van enkele grootheden."
2220 PRINT"Deze grootheden zijn:"
2230 PRINT
2240 PRINT" - vruchtbaarheid gehuwde vrouwen"
2250 PRINT" - vruchtbaarheid ongehuwde vrouwen"
2260 PRINT" - verhouding jongens/meisjes bij"
2270 PRINT"   geboorte"
2280 PRINT" - zuigelingensterfte"
2290 PRINT" - emigratie"
2300 PRINT" - immigratie"
2310 PRINT:PRINT
2320 PRINT"      (druk op toets voor vervolg)";
2330 GOSUB210
2340 GOSUB100
2350 PRINT"Door de waarden van deze grootheden"
2360 PRINT"te varieren kunt u de invloed op de"
2370 PRINT"bevolkingsontwikkeling onderzoeken."
2380 PRINT:PRINT
2390 PRINT"Per jaar wordt op het scherm in de"
2400 PRINT"vorm van een bevolkingspiramide de"
2410 PRINT"berekende bevolkingsopbouw gegeven."
2420 PRINT"Een jaar berekenen kost ca. 15 sek."
2430 PRINT
2440 PRINT"Indien u tijdens het rekenen op de"
2450 PRINT"toets van de letter T drukt krijgt u"
2460 PRINT"tevens de gegevens in tabelvorm."
2470 PRINT:PRINT
2480 PRINT"  (druk op toets voor vervolg)";
2490 GOSUB210
2500 RETURN
2510 REM
2520 REM *****************************
2530 REM * INVOER VAN VARIABELE WAAR-*
2540 REM * DEN BEVOLKINGSGEGEVENS    *
2550 REM *****************************
2560 REM
2570 GOSUB100
2580 PRINT"U kunt nu de waarden van de genoemde"
2590 PRINT"grootheden opgeven door na elke vraag"
2600 PRINT"een percentage in te tikken."
2610 PRINT"De waarde van die grootheid wordt dan"
2620 PRINT"gelijk aan het overeenkomstige percen-"
2630 PRINT"tage van de waarde van ";J0;"."
2640 PRINT"Dus intikken van bv. 100, 90 of 110"
2650 PRINT"betekent rekenen met een waarde gelijk"
2660 PRINT"aan, een 10 procent lagere of een 10"
2670 PRINT"procent hogere waarde dan ";J0;"."
2680 PRINT
2690 PRINT"Een uitzondering vormt de vraag naar"
2700 PRINT"het percentage jongens bij geboorte."
2710 PRINT"U moet hier met het gewenste percen-"
2720 PRINT"tage zelf antwoorden!"
2730 PRINT:PRINT
2740 PRINT"      (druk op toets voor vervolg)";
2750 GOSUB210
2760 IN$=""
2770 GOSUB100
2780 PRINT"Geef het percentage van de waarde van"
2790 PRINTJ0;" van:"
2800 PRINT:PRINT
2810 PRINT"Vruchtbaarheid gehuwde vrouwen"
2820 INPUTC1
2830 C1=C1/100
2840 PRINT"Vruchtbaarheid ongehuwde vrouwen"
2850 INPUTC2
2860 C2=C2/100
2870 PRINT"Zuigelingensterfte"
2880 INPUTC3
2890 C3=C3/100
2900 PRINT"Aantal emigranten"
2910 INPUTC4
2920 C4=C4/100
2930 PRINT"Aantal immigranten"
2940 INPUTC5
2950 C5=C5/100
2960 PRINT:PRINT
2970 PRINT"Geef nu het percentage jongens van de"
2980 PRINT"levend geboren baby's"
2990 PRINT"(in ";J0;" was dit ";LM*100;")"
3000 INPUTC6
3010 C6=C6/100
3020 IF(C6>=0) AND(C6<=1) THEN3050
3030 PRINT"Geef een percentage tussen 0 en 100"
3040 GOTO3000
3050 FORI=1 TO18
3060 LG(I)=C1*LG(I)
3070 LO(I)=C2*LO(I)
3080 EM(I)=C4*EM(I)
3090 EV(I)=C4*EV(I)
3100 IM(I)=C5*IM(I)
3110 IV(I)=C5*IV(I)
3120 NEXTI
3130 SM(0)=C3*SM(0)
3140 SV(0)=C3*SV(0)
3150 LM=C6
3160 GOSUB100
3170 PRINT"Tot welk jaar wilt u een prognose"
3180 INPUTJD
3190 IFJD>J0 THEN3220
3200 PRINT"Geef een jaartal na ";J0
3210 GOTO3180
3220 RETURN
