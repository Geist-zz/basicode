1030 REM * DIAGRAM VAN STARTGEGEVENS *
1040 REM *****************************
1050 REM
1060 GOSUB3900:REM * INITIALISATIE *
1070 GOSUB2010:REM * UITLEG *
1080 GOSUB2510:REM * INVOER VARIABELE WAARDEN *
1090 AM=AM(16):AV=AV(16)
1100 AM(16)=AM(16)+AM(17)+AM(18)
1110 AV(16)=AV(16)+AV(17)+AV(18)
1120 GOSUB3230:REM * GRAFISCHE WEERGAVE *
1130 AM(16)=AM:AV(16)=AV
1140 REM
1150 REM *****************************
1160 REM * HOOFDPROGRAMMA MET BERE-  *
1170 REM * KENING VAN DE ONTWIKKE-   *
1180 REM * LING VAN DE BEVOLKING PER *
1190 REM * JAAR                      *
1200 REM *****************************
1210 REM
1220 REM * BEVOLKINGSAANTALLEN PER LEEFTIJD *
1230 FORI=1 TO17
1240 FORJ=0 TO4
1250 K=5*(I-1)+J
1260 M(K)=INT(AM(I)/5+.5)
1270 V(K)=INT(AV(I)/5+.5)
1280 NEXTJ
1290 NEXTI
1300 M(85)=AM(18):V(85)=AV(18)
1310 REM * BEVOLKINGSONTWIKKELING *
1320 REM * 85 JARIGEN EN OUDER *
1330 M(85)=INT((M(85)+IM(18)-EM(18))*(1-SM(18)/1000)+.5)
1340 V(85)=INT((V(85)+IV(18)-EV(18))*(1-SV(18)/1000)+.5)
1350 REM * 0-84 JARIGEN STANDAARD *
1360 FORI=1 TO17
1370 FORJ=0 TO4
1380 K=5*(I-1)+J
1390 M(K)=INT((M(K)+(IM(I)-EM(I))/5)*(1-SM(I)/1000)+.5)
1400 V(K)=INT((V(K)+(IV(I)-EV(I))/5)*(1-SV(I)/1000)+.5)
1410 NEXTJ
1420 NEXTI
1430 REM * CORRECTIE ZUIGELINGENSTERFTE *
1440 M(0)=INT(M(0)-M(0)*SM(0)/(1000-SM(1))+.5)
1450 V(0)=INT(V(0)-V(0)*SV(0)/(1000-SV(1))+.5)
1460 REM * EEN JAAR DOORSCHUIVEN *
1470 JR=JR+1
1480 M(85)=M(85)+M(84)
1490 V(85)=V(85)+V(84)
1500 FORI=84 TO1 STEP-1
1510 M(I)=M(I-1)
1520 V(I)=V(I-1)
1530 NEXTI
1540 REM * AANTAL GEBOORTEN *
1550 AG=0
1560 FORI=1 TO18
1570 AG=AG+AV(I)*(GH(I)*LG(I)+(1-GH(I))*LO(I))
1580 NEXTI
1590 M(0)=INT(AG*LM+.5)
1600 V(0)=INT(AG-M(0)+.5)
1610 REM * SOMMEREN PER 5-JAARSKLASSEN *
1620 FORI=1 TO18
1630 AM(I)=0:AV(I)=0
1640 NEXTI
1650 FORI=1 TO17
1660 FORJ=0 TO4
1670 K=5*(I-1)+J
1680 AM(I)=AM(I)+M(K)
1690 AV(I)=AV(I)+V(K)
1700 NEXTJ
1710 NEXTI
1720 AM(16)=AM(16)+AM(17)+M(85)
1730 AV(16)=AV(16)+AV(17)+V(85)
1740 REM * UITVOER ACTUELE DATA *
1750 IFJR>=JD THEN1920
1760 IFIN$="" THEN1820
1770 REM * VERVANGING VAN TABEL DOOR DIAGRAM *
1780 HO=5:VE=23:GOSUB110:REM * PLAATS OP SCHERM *
1790 PRINT"(Druk op toets voor vervolg)";
