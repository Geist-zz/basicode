3620 IF(IN$="j") OR(IN$="J") THEN3640
3630 GOTO3600:REM  geen j of nee 
3640 IF(C<0) OR(A<0) THENGOSUB10135:GOTO3540
3650 IF(C<A) THENGOSUB10150:GOTO3540
