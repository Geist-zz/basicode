3750 VE=19:GOSUB110:PRINT"B^2 =";C^2-A^2
3760 VE=20:GOSUB110:PRINT"B   =";L
3770 HO=17:VE=7:GOSUB110:PRINT"A =";A
3780 VE=8:GOSUB110:PRINT"B =";L
