
1000 A=5500:GOTO20
1010 REM  MCTOETS  (Multiple-choicetest, 10 K)
1020 T=75:DIML(T),R(T),A$(T),B$(T,4)
1030 P=ASC("A")-1:Q=ASC("a")-1
1100 GOSUB100:FORV=1 TOT:R(V)=0:NEXTV:F=0:G=0
1110 PRINT"**  DE ";T;"- WOORDEN - TEST  **":PRINT
1120 PRINT:PRINT"Bij elk van de moeilijke woorden"
1130 PRINT"vind je 4 mogelijke betekenissen.":PRINT
1140 PRINT"Kies telkens de goede letter (A-B-C-D).":PRINT
1200 FORV=1 TOT:READA$(V):IF(V<10) THENPRINT" ";
1210 PRINTV;" ";:IFV/7=INT(V/7) THENPRINT
1300 GOSUB260:I=INT(RV*T+1):IFR(I)>0 THEN1300
1310 R(I)=V:FORJ=1 TO4:READB$(V,J):NEXTJ:READL
1320 L(V)=L:NEXTV:GOSUB1800:FORV=1 TOT:I=R(V)
1330 GOSUB100:PRINT"Score : ";G;" goed  en ";F;" fout"
1340 PRINT:PRINT"Vraag ";V;" :  ";A$(I):FORJ=1 TO5
1350 L=L(I):GOSUB260:A=INT(4*RV+1):L(I)=A
1360 B$=B$(I,L):B$(I,L)=B$(I,A):B$(I,A)=B$:NEXTJ
1400 PRINT:FORJ=1 TO4:PRINTCHR$(J+P);")   ";B$(I,J)
1410 NEXTJ:PRINT:PRINT"Wat kies je (A, B, C of D) ? ";
1500 GOSUB1900:L=0
1510 IF(IN$>="A") AND(IN$<="D") THENL=ASC(IN$)-P
1520 IF(IN$>="a") AND(IN$<="d") THENL=ASC(IN$)-Q
1530 IFL=0 THEN1500
1540 PRINTIN$;"  ";:IFL=L(I) THENPRINT"(JUIST)":G=G+1
1550 IFL<>L(I) THENPRINT"(FOUT)":F=F+1:GOSUB250
1560 PRINT:PRINT:PRINTA$(I);" = ";B$(I,L(I))
1570 GOSUB1800:NEXTV:V=T:PRINT:PRINT
1700 PRINT"Totale score uit ";V;" items : ";G:PRINT
1710 PRINT"     <Enter> ";:INPUTIN$:GOSUB100:END
1800 PRINT:PRINT:PRINT"     <toets> ";
1900 GOSUB200:IFIN$<>"" THEN1900
1910 GOSUB210:RETURN
