28790 DATA". 2   G   C"
28800 DATA". 3   A   U    Ala  A    Alanine"
28810 DATA". 4   T   A"
28820 DATA". 5   A   U"
28830 DATA". 6   A   U    Ile  I    Isoleucine"
28840 DATA". 7   C   G"
28850 DATA". 8   T   A"
