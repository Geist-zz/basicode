1070 GOSUB210:IFIN=48 THEN950
1080 IF(IN<50)OR(IN>53) THEN1070
1090 AA=VAL(IN$):PRINTAA
1100 RESTORE:FORN=1 TO20:READB$(N):NEXTN
1110 REM -- schudden --
1120 FORN=1 TO20:Z(N+1)=N:NEXTN
1130 FORN=1 TO19:GOSUB260:X=N+1+INT(RV*(21-N)):Z(N)=Z(X)
1140 FORM=X-1 TON+1 STEP-1:Z(M+1)=Z(M):NEXTM:NEXTN
1150 Z(20)=Z(21)
1160 REM -- regelarrays --
1170 FORN=1 TOAA:A(N+1)=Z(N):B(N+1)=Z(N):C(N+1)=Z(N):NEXTN
1180 FORN=1 TOAA-1:GOSUB260:X=N+1+INT(RV*(AA+1-N)):A(N)=A(X)
1190 FORM=X-1 TON+1 STEP-1:A(M+1)=A(M):NEXTM:NEXTN
1200 A(AA)=A(AA+1)
1210 FORN=1 TOAA-1:GOSUB260:X=N+1+INT(RV*(AA+1-N)):B(N)=B(X)
1220 FORM=X-1 TON+1 STEP-1:B(M+1)=B(M):NEXTM:NEXTN
1230 B(AA)=B(AA+1)
1240 FORN=1 TOAA-1:GOSUB260:X=N+1+INT(RV*(AA+1-N)):C(N)=C(X)
1250 FORM=X-1 TON+1 STEP-1:C(M+1)=C(M):NEXTM:NEXTN
1260 C(AA)=C(AA+1)
1270 REM -- het foute plaatje ---
1280 GOSUB260:F=1+INT(RV*3*AA):PL=F:RY=1
1290 IFPL>AA THENPL=PL-AA:RY=RY+1:GOTO1290
1300 ONRY GOTO1310,1320,1330
1310 A(PL)=Z(20):GOTO1340
1320 B(PL)=Z(20):GOTO1340
1330 C(PL)=Z(20)
1340 FH=.012:FV=.016:HC=.075:HM=HC:VC=.42:VM=VC:GOSUB600
