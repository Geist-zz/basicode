2000 REM == subr. teken plaatje == T$
2010 P=1:GOSUB2510:BG=V:IFBG=0 THEN2110
2020 REM --- cirkelbogen ---
2030 FORCC=1 TOBG
2040 GOSUB2510:XC=V:GOSUB2510:YC=V:GOSUB2510:RC=V
2050 GOSUB2510:BC=V:GOSUB2510:EC=V
2060 GOSUB2310:NEXTCC
2100 REM --- rechte lijnen ---
2110 GOSUB2510:LP=V:IFLP=0 THEN2180
2120 FORS=1 TOLP
2130 GOSUB2510:M=V
2140 GOSUB2510:HO=XX+FH*V:GOSUB2510:VE=YY+FV*V:GOSUB620
2150 FOREM=1 TOM
2160 GOSUB2510:HO=XX+FH*V:GOSUB2510:VE=YY+FV*V:GOSUB630
2170 NEXTEM:NEXTS
2180 RETURN
2300 REM == subr. teken cirkelboog == XC,YC,RC,BC,EC
2310 BC=2*BC:EC=2*EC:IFEC<=BC THENEC=EC+16
2320 HO=XX+FH*(XC+RC*S(BC+4)):VE=YY+FV*(YC-RC*S(BC))
2330 GOSUB620
2340 FOREM=BC+1 TOEC:HK=EM:IFHK>16 THENHK=EM-16
2350 HO=XX+FH*(XC+RC*S(HK+4)):VE=YY+FV*(YC-RC*S(HK))
2360 GOSUB630
2370 NEXTEM:RETURN
2500 REM == subr. lees tekendata == T$,P / V
2510 V=VAL(MID$(T$,P,1)):P=P+1:RETURN
2600 REM == subr. cursor ==
