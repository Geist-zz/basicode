1450 XX=.05:YY=YY+.25:FORTK=1 TOAA:T$=B$(B(TK))
1460 GOSUB2010:XX=XX+.2:NEXTTK
1470 XX=.05:YY=YY+.25:FORTK=1 TOAA:T$=B$(C(TK))
1480 GOSUB2010:XX=XX+.2:NEXTTK
